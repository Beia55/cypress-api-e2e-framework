{
  "name": "cypress-framework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:api": "cypress run --spec=cypress/tests/api/**/*.spec.js",
    "cy:e2e": "cypress run --spec=cypress/tests/e2e/**/*.spec.js --browser electron",
    "cy:e2e:chrome": "cypress run --spec=cypress/tests/e2e/**/*.spec.js --browser chrome",
    "cy:e2e:edge": "cypress run --spec=cypress/tests/e2e/**/*.spec.js --browser edge",
    "cy:e2e:firefox": "cypress run --spec=cypress/tests/e2e/**/*.spec.js --browser firefox", 
    "cy:update-snapshots": "cypress run --env updateSnapshots=true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "4.5.0",
    "cypress-image-snapshot": "4.0.1",
    "prettier": "2.8.1"
  },
  "include":["./jsconfig.json"]
}
